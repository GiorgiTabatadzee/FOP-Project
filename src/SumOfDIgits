public class SumOfDigit {
    public static void main(String[] args) {

        String code = """
         var number = 293847
         var sumOfDigits = 0
        
         while (number > 0) {
         var digit = number % 10
         
         sumOfDigits += digit
         number = number / 10
         }
        
        println(sumOfDigits)
                
                
        """;

         KotlinToJavaInterpreterMain interpreter = new KotlinToJavaInterpreterMain();
        interpreter.interpret(code);
    }
}

