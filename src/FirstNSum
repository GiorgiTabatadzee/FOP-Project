public class FirstNSum {
    public static void main(String[] args) {

        String code = """
                var n = 15
                var sum = 0
                var i = 1
                while (i <= n) {
                sum += i
                i += 1
                 }
                println(sum)     
        """;

        KotlinToJavaInterpreter interpreter = new KotlinToJavaInterpreter();
        interpreter.interpret(code);
    }
}
